    "class UserInputValidator:\n",
    "    def validate_positive_integers(list):\n",
    "        final_list = []\n",
    "        for string in list:\n",
    "            if string.isdigit():\n",
    "                final_list.append(string)\n",
    "\n",
    "        return print(f\"The list contains {len(final_list)} integer(s) \")\n",
    "\n",
